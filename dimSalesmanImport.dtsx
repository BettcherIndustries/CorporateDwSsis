          SQLTask:Connection="{29A0C9FD-F565-44E7-B050-51AD8DC77ECB}"
          SQLTask:SqlStatementSource="&#xA;&#xA;--check LTG&#xA;Begin Transaction;&#xA;if not exists(SELECT     SalesmanKeyAlternative&#xA;FROM         dimSalesman&#xA;WHERE     (SiteKey = 3) AND (SalesmanKeyAlternative = N'T00'))&#xA;Insert into dimSalesman&#xA;(SiteKey, SiteKeyAlternative,  SalesmanKeyAlternative, Salesman)&#xA;Values&#xA;(3,'LTG','T00', 'NO SALESMAN ASSIGNED')&#xA;Commit Transaction;&#xA;&#xA;&#xA;&#xA;--check GAN&#xA;Begin Transaction;&#xA;if not exists(SELECT     SalesmanKeyAlternative&#xA;FROM         dimSalesman&#xA;WHERE     (SiteKey = 2) AND (SalesmanKeyAlternative = N'T00'))&#xA;Insert into dimSalesman&#xA;(SiteKey, SiteKeyAlternative,  SalesmanKeyAlternative, Salesman)&#xA;Values&#xA;(2,'GAN','T00', 'NO SALESMAN ASSIGNED')&#xA;Commit Transaction;&#xA;&#xA;--check BET&#xA;Begin Transaction;&#xA;if not exists(SELECT     SalesmanKeyAlternative&#xA;FROM         dimSalesman&#xA;WHERE     (SiteKey = 1) AND (SalesmanKeyAlternative = N'T00'))&#xA;Insert into dimSalesman&#xA;(SiteKey, SiteKeyAlternative,  SalesmanKeyAlternative, Salesman)&#xA;Values&#xA;(1,'BET','T00', 'NO SALESMAN ASSIGNED')&#xA;Commit Transaction;&#xA;&#xA;&#xA;&#xA;--check MED&#xA;Begin Transaction;&#xA;if not exists(SELECT     SalesmanKeyAlternative&#xA;FROM         dimSalesman&#xA;WHERE     (SiteKey = 4) AND (SalesmanKeyAlternative = N'T00'))&#xA;Insert into dimSalesman&#xA;(SiteKey, SiteKeyAlternative,  SalesmanKeyAlternative, Salesman)&#xA;Values&#xA;(4,'MED','T00', 'NO SALESMAN ASSIGNED')&#xA;Commit Transaction;&#xA;&#xA;--check BSW&#xA;Begin Transaction;&#xA;if not exists(SELECT     SalesmanKeyAlternative&#xA;FROM         dimSalesman&#xA;WHERE     (SiteKey = 5) AND (SalesmanKeyAlternative = N'T00'))&#xA;Insert into dimSalesman&#xA;(SiteKey, SiteKeyAlternative,  SalesmanKeyAlternative, Salesman)&#xA;Values&#xA;(5,'BSW','T00', 'NO SALESMAN ASSIGNED')&#xA;Commit Transaction;&#xA;&#xA;&#xA;--check BBR&#xA;Begin Transaction;&#xA;if not exists(SELECT     SalesmanKeyAlternative&#xA;FROM         dimSalesman&#xA;WHERE     (SiteKey =6) AND (SalesmanKeyAlternative = N'T00'))&#xA;Insert into dimSalesman&#xA;(SiteKey, SiteKeyAlternative,  SalesmanKeyAlternative, Salesman)&#xA;Values&#xA;(6,'BBR','T00', 'NO SALESMAN ASSIGNED')&#xA;Commit Transaction;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
