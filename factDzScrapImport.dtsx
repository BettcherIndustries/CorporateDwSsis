                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT   LTRIM(job.job) AS Job, job.suffix AS Suffix, jobroute_mst.oper_num, ISNULL(ItemJobMaterial.Cost, 0) AS ItemJobMaterialCost, ISNULL(prodcode.unit, NULL) AS AcctUnit2, ISNULL(job.type, 'J') AS Type, 'SCRAP ' + LTRIM(job.job) + '||' + CAST(job.suffix AS nvarchar(5)) AS JournalRef, item.item, 
             job.Uf_JobCompleteDate AS JobCloseDate, DATEADD(dd, DATEDIFF(dd, 0, job.Uf_PostScrapJournalDate), 0) AS PostScrapJournalDate, CASE WHEN Uf_PostScrapJournalDate IS NOT NULL THEN 1 ELSE 0 END AS Posted, ISNULL(job.rework, 0) AS Rework, jobroute_mst.qty_scrapped AS QtyScrap, 
             ROUND(SUM(ISNULL(jobroute_mst.qty_scrapped, 0)) * ISNULL(ItemJobMaterial.Cost, 0), 2) AS ScrapDollars, jobroute_mst.wc
FROM     item_mst AS item INNER JOIN
             job_mst AS job ON item.item = job.item INNER JOIN
             jobroute_mst ON job.job = jobroute_mst.job AND job.suffix = jobroute_mst.suffix AND job.site_ref = jobroute_mst.site_ref LEFT OUTER JOIN
             prodcode_mst AS prodcode ON item.product_code = prodcode.product_code LEFT OUTER JOIN
                 (SELECT   job, suffix, SUM(matl_qty * cost_conv) AS Cost
                 FROM     jobmatl_mst AS jobmatl
                 WHERE   (matl_type = N'M')
                 GROUP BY job, suffix) AS ItemJobMaterial ON item.job = ItemJobMaterial.job AND item.suffix = ItemJobMaterial.suffix
GROUP BY job.job, job.suffix, job.item, item.description, item.product_code, ItemJobMaterial.Cost, job.qty_scrapped, job.stat, job.wip_acct_unit1, job.wip_acct_unit2, job.wip_acct_unit3, job.wip_acct_unit4, job.type, item.item, prodcode.unit, job.Uf_JobCompleteDate, item.item, job.Uf_PostScrapJournalDate, job.rework, 
             jobroute_mst.oper_num, jobroute_mst.qty_scrapped, jobroute_mst.wc
HAVING   (job.stat = N'C') AND (ISNULL(job.rework, 0) = 0) AND (job.suffix NOT LIKE '%-1%') AND (SUM(job.qty_scrapped) &gt; 0) AND (dbo.MidnightOf(job.Uf_JobCompleteDate) BETWEEN '05/01/2017' AND dbo.MidnightOf(GETDATE()))</property>
