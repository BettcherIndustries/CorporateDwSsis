	,COALESCE(item1,item.item+'-'+item.description) as ItemNoDescription
	,isnull(job.qty_scrapped,(MaltTranQty*-1)) as Qty
	
	FROM         item_mst item RIGHT OUTER JOIN
                      job_mst job ON item.item = job.item RIGHT OUTER JOIN
                      cteReport ON ltrim(job.job) = cteReport.job 
