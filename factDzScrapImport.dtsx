                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH cteReport 
AS	
(
SELECT     LG.acct
		, CH.description AS AcctDesc
		, LG.dom_amount, CASE WHEN LG.dom_amount &gt; 0 THEN LG.dom_amount ELSE 0 END AS 'Debit'
		, CASE WHEN LG.dom_amount &lt; 0 THEN LG.dom_amount ELSE 0 END AS 'Credit'
		, LG.from_id
		, LG.ref
		, CONVERT (DATE, LG.trans_date) AS 'GLTranDate'
		, LG.trans_num AS 'GLTranNum'
		, LG.matl_trans_num
		, ML.item
		, ML.item + '-' + item.description  AS Item1
		, case when LG.from_id ='Scrap'
			Then ltrim(replace(substring(LTRIM(LG.ref),0,CHARINDEX('||',LTRIM(LG.ref))),'Scrap',''))
			Else LTRIM(ML.ref_num) 
			End AS Job
		, case when LG.from_id ='Scrap'
			Then substring(LTRIM(LG.ref),CHARINDEX('||',LTRIM(LG.ref))+2,4)
			Else ML.ref_line_suf 
			End AS Suffix
		, ML.ref_release AS Oper
		, ML.wc
		, ML.user_code 
        ,ML.reason_code
        , ML.document_num
		,ML.qty as MaltTranQty
		 ,item.product_code 
		

       
FROM            item_mst AS item RIGHT OUTER JOIN
                         matltran_mst AS ML ON item.item = ML.item RIGHT OUTER JOIN
                         ledger_mst AS LG LEFT OUTER JOIN
                         chart_mst AS CH ON LG.acct = CH.acct ON ML.trans_num = LG.matl_trans_num

WHERE     (LG.control_year in (2016,2017)) 
	AND (LG.control_period  in (1,2,3,4))
	
	AND LG.acct in ('3710') 
	
	
)



