                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT   1 as DivId,   CONVERT (DATE, LG.trans_date) AS 'GLTranDate',  CASE WHEN LG.from_id = 'Scrap' and CHARINDEX('||', LTRIM(lg.ref)) &gt; 0 THEN ltrim(replace(substring(LTRIM(LG.ref), 0, CHARINDEX('||', LTRIM(LG.ref))), 'Scrap', '')) ELSE LTRIM(ref) END AS Job, 
                         CASE WHEN LG.from_id = 'Scrap' and CHARINDEX('||', LTRIM(lg.ref))&gt; 0 THEN REPLACE(SUBSTRING(LTRIM(lg.ref), CHARINDEX('||', LTRIM(lg.ref)), 5), '||', '') ELSE 0 END AS Suffix, lg.dom_amount, lg.ref, jr.oper_num, ItemJobMaterial.Cost, 
                         jt.qty_scrapped, LTRIM(jt.emp_num) AS emp_num, lg.from_id ,  ROW_NUMBER() OVER(PARTITION BY j.job 
                                 ORDER BY j.job DESC) AS Rank, isnull(jt.shift,1) as shift, j.Uf_JobCompleteDate, j.Uf_PostScrapJournalDate, isnull(j.item,'00000') as item, jr.wc, jt.Uf_Resid



FROM            jobroute_mst  jr INNER JOIN
                         job_mst  j ON jr.job = j.job AND jr.suffix =j.suffix AND jr.site_ref = j.site_ref INNER JOIN
                         jobtran_mst jt ON jr.site_ref = jt.site_ref AND jr.job = jt.job AND jt.suffix = jt.suffix AND 
                         jr.oper_num =jt.oper_num RIGHT OUTER JOIN
                         ledger_mst AS lg ON j.suffix = CASE WHEN LG.from_id = 'Scrap' and CHARINDEX('||', LTRIM(lg.ref))&gt;0 THEN REPLACE(SUBSTRING(LTRIM(lg.ref), CHARINDEX('||', LTRIM(lg.ref)), 5), '||', '') ELSE 0 END AND j.site_ref = lg.site_ref AND 
                         CASE WHEN LG.from_id = 'Scrap' THEN ltrim(replace(substring(LTRIM(LG.ref), 0, CHARINDEX('||', LTRIM(LG.ref))), 'Scrap', '')) ELSE LTRIM(ref) END = LTRIM(j.job) LEFT OUTER JOIN
                             (SELECT        job, suffix, SUM(matl_qty * cost_conv) AS Cost
                               FROM            jobmatl_mst AS jobmatl
                               WHERE        (matl_type = N'M')
                               GROUP BY job, suffix) AS ItemJobMaterial ON j.job = ItemJobMaterial.job AND j.suffix = ItemJobMaterial.suffix
WHERE        (lg.control_year IN (2016,2017)) AND (lg.acct IN ('3710'))</property>
