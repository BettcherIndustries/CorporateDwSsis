                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT   ltrim(jobroute_mst.job) as Job
		, jobroute_mst.suffix as Suffix
		, job_mst.item as Item
		, item_mst.description as ItemDescription
		, jobroute_mst.oper_num as OperNum
		,DATEADD(dd, DATEDIFF(dd, 0, specificnotes.CreateDate), 0)  as TransDate
		, CONVERT(VARCHAR(5), specificnotes.CreateDate, 108) as TransTime
		,NULL as TransType
		,substring(replace(substring(SpecificNotes.NoteContent ,CHARINDEX('| Oper:',SpecificNotes.NoteContent),500),'| Oper:',''),0,charindex('-',replace(substring(SpecificNotes.NoteContent ,CHARINDEX('| Oper:',SpecificNotes.NoteContent),500),'| Oper:',''))) AS  EmpNum
		, employee_mst.name as Employee
		,0 as QtyScrap
		--,CAST(SUBSTRING(SpecificNotes.NoteContent, CHARINDEX('| Qty:', SpecificNotes.NoteContent) + 6, 6) AS decimal(19, 4)) AS QtyScrapNote
		,REPLACE(SUBSTRING(REPLACE(SUBSTRING(NoteContent, CHARINDEX('Qty:', NoteContent), 10), 'Qty:', ''), 1, CHARINDEX('.', REPLACE(SUBSTRING(NoteContent, CHARINDEX('Qty:', 
                         NoteContent), 10), 'Qty:', ''))), '.', '') * 1  AS QtyScrapNote
		,REPLACE(SUBSTRING(NoteContent, CHARINDEX('Reason:', NoteContent), CHARINDEX('| Qty:', NoteContent) - CHARINDEX('Reason:', 
                         NoteContent)), 'Reason:', '') as Reason
		--,substring(SpecificNotes.NoteContent  ,CHARINDEX('Reason:', SpecificNotes.NoteContent)+7,CHARINDEX('| Qty:',substring(SpecificNotes.NoteContent  ,CHARINDEX('Reason:', SpecificNotes.NoteContent)+7,500))-1) as Reason
		, SUBSTRING(SpecificNotes.NoteContent, 0, CHARINDEX('|', SpecificNotes.NoteContent)) as NoteContent
		, cast(SpecificNotes.NoteContent as nvarchar(250)) as NoteContentFull
			 ,isnull(item_mst.unit_cost,0) as UnitCost 
		   , isnull(item_mst.Uf_UnitCost1,0) as UnitCost1
		   , isnull(item_mst.Uf_UnitCost2,0) as UnitCost2
		   , isnull(item_mst.Uf_UnitCost3,0) as UnitCost3
		   , isnull(item_mst.Uf_UnitCost4,0) as UnitCost4
		   , isnull(item_mst.Uf_UnitCost5,0) as UnitCost5
		   , isnull(item_mst.Uf_UnitCost6,0) as UnitCost6
		   , isnull(item_mst.Uf_UnitCost7,0) as UnitCost7
		   , isnull(item_mst.Uf_UnitCost8,0) as UnitCost8
		   , isnull(item_mst.Uf_UnitCost9,0) as UnitCost9
		FROM     ObjectNotes INNER JOIN
             SpecificNotes ON ObjectNotes.SpecificNoteToken = SpecificNotes.SpecificNoteToken INNER JOIN
             jobroute_mst ON ObjectNotes.RefRowPointer = jobroute_mst.RowPointer INNER JOIN
             job_mst ON jobroute_mst.job = job_mst.job AND jobroute_mst.suffix = job_mst.suffix AND jobroute_mst.site_ref = job_mst.site_ref LEFT OUTER JOIN
             item_mst ON job_mst.site_ref = item_mst.site_ref AND job_mst.item = item_mst.item LEFT OUTER JOIN
             employee_mst ON SUBSTRING(REPLACE(SUBSTRING(SpecificNotes.NoteContent, CHARINDEX('| Oper:', SpecificNotes.NoteContent), 500), '| Oper:', ''), 0, CHARINDEX('-', REPLACE(SUBSTRING(SpecificNotes.NoteContent, CHARINDEX('| Oper:', SpecificNotes.NoteContent), 500), '| Oper:', ''))) = LTRIM(employee_mst.emp_num)
WHERE   (ObjectNotes.NoteHeaderToken = 23)
 and (dbo.MidnightOf(SpecificNotes.CreateDate)&gt;='03/01/2016')
  AND (dbo.MidnightOf(SpecificNotes.CreateDate) between '01/01/2017' and '03/31/2017')</property>
