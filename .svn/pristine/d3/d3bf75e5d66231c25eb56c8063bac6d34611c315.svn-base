                      5 AS SiteKey, customer.ship_site, LTRIM(custaddr.cust_num) AS cust_num, custaddr.cust_seq,rtrim(ltrim(custaddr.name)) AS Customer, ISNULL(LTRIM(custaddr.corp_cust), LTRIM(custaddr.cust_num)) AS corp_cust, 
