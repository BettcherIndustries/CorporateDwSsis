                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT 
                      3 AS SiteKey, customer.ship_site, LTRIM(custaddr.cust_num) AS cust_num, custaddr.cust_seq, custaddr.name + '(' + ISNULL(custaddr.city, N'') 
                      + ', ' + ISNULL(custaddr.state, N'') + ')  (' + LTRIM(custaddr.cust_num) + ')' AS Customer, ISNULL(LTRIM(custaddr.corp_cust), LTRIM(custaddr.cust_num)) AS corp_cust, 
                      ISNULL(custaddr.addr##1, N'') + ', ' + ISNULL(custaddr.addr##2, N'') + ISNULL(custaddr.city, N'') + ', ' + ISNULL(custaddr.state, N'') AS CustAddr, 
                      isnull(customer.territory_code,0) as territory_code, 
                      isnull(customer.tax_code1,'NT') as tax_code1

FROM         custaddr INNER JOIN
                      customer ON custaddr.cust_num = customer.cust_num AND custaddr.cust_seq = customer.cust_seq
WHERE     (custaddr.cust_seq = 0)
