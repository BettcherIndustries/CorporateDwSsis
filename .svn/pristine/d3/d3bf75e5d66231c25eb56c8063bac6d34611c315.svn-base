                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT 
                      1 AS SiteKey, cast('BET' as nvarchar(5)) as ship_site, LTRIM(custaddr.cust_num) AS cust_num, custaddr.cust_seq, rtrim(ltrim(custaddr.name))  AS Customer, ISNULL(LTRIM(custaddr.corp_cust), LTRIM(custaddr.cust_num)) AS corp_cust, 
                      ISNULL(rtrim(ltrim(custaddr.addr##1)), N'') + ', ' + ISNULL(rtrim(ltrim(custaddr.addr##2)), N'') + ISNULL(rtrim(ltrim(custaddr.city)), N'') + ', ' + ISNULL(rtrim(ltrim(custaddr.state)), N'') AS CustAddr, 
                      isnull(customer.territory_code,0) as territory_code, rtrim(ltrim(custaddr.name)) + '(' + ISNULL(rtrim(ltrim(custaddr.city)), N'') 
