                      tblForecastMfgImport.ForecastQty, 0 AS AvailQty, dimItem.ItemFamilyCodeKey, dimItem.ItemCommCodeKey, dimItem.ItemPmtCodeKey, dimItem.ProdCodeSlKey, 
