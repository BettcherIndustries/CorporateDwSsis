                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with cteData
as
(

SELECT         DimTime.MonthDate AS Date, DimTime.MonthDate, DimTime.Year, DimTime.QuarterDate, DimTime.YearDate,  dimSiteLevelKey.Level2Key, dimSiteLevelKey.Level2Description, dimSiteLevelKey.Level1Key, dimSiteLevelKey.Level1Description, 
                         dimSiteLevelKey.Level3Description AS Division, dimProduct.ProdLineKey, dimProduct.ProdLineDescription AS ProdLine, dimProduct.CategoryKey, dimProduct.CategoryDescription AS Category, 
                         dimProduct.ProdGroupKey, dimProduct.ProdGroupDescription AS Prodgrp, dimItemFamilyCode.ItemFamilyCodeDescription AS Family, dimSalesTerritory.SalesTerritoryKey, 
                         dimSalesTerritory.SalesTerritoryDescription AS Territory, dimGeographic.DomesticForeign, dimGeographic.State, dimGeographic.CountryCode, dimGeographic.SubRegionKeyAlternative as SubRegion, 
                         dimGeographic.RegionKeyAlternative as Region, SUM(factInvoiceSales.ExtCorpListprice) AS GrossSales, 0 AS AopSales
FROM            factInvoiceSales INNER JOIN
                         DimTime ON factInvoiceSales.InvoiceDateKey = DimTime.TimeKey INNER JOIN
                         dimSiteLevelKey ON factInvoiceSales.SiteKey = dimSiteLevelKey.LevelId INNER JOIN
                         dimProduct ON factInvoiceSales.ProdCodeSlKey = dimProduct.ProdCodeSlKey INNER JOIN
                         dimSalesTerritory ON factInvoiceSales.SalesTerritoryKey = dimSalesTerritory.SalesTerritoryKey INNER JOIN
                         dimGeographic ON factInvoiceSales.GeographicKey = dimGeographic.GeographicKey INNER JOIN
                         dimItemFamilyCode ON factInvoiceSales.ItemFamilyCodeKey = dimItemFamilyCode.ItemFamilyCodeKey LEFT OUTER JOIN
                         dimOrderEndType ON factInvoiceSales.OrderEndKey = dimOrderEndType.OrderEndKey
WHERE        (factInvoiceSales.SiteKey IN (1, 2, 4))
GROUP BY DimTime.MonthDate, DimTime.Year, DimTime.QuarterDate,  DimTime.YearDate, dimSiteLevelKey.Level2Key, dimSiteLevelKey.Level2Description, dimSiteLevelKey.Level1Key, dimSiteLevelKey.Level1Description, dimOrderEndType.OrderEndDescription,
                          dimSiteLevelKey.Level3Description, dimProduct.ProdLineKey, dimProduct.ProdLineDescription, dimProduct.CategoryKey, dimProduct.CategoryDescription, dimProduct.ProdGroupKey, 
                         dimProduct.ProdGroupDescription, dimSalesTerritory.SalesTerritoryKey, dimSalesTerritory.SalesTerritoryDescription, dimGeographic.DomesticForeign, dimGeographic.State, dimGeographic.CountryCode, 
                         dimGeographic.SubRegionKeyAlternative, dimGeographic.RegionKeyAlternative, dimItemFamilyCode.ItemFamilyCodeDescription 
						 HAVING        (DimTime.[Year] &gt;= 2015) AND (dimOrderEndType.OrderEndDescription = N'Sales')
Union All
SELECT       DimTime.MonthDate AS Date, DimTime.MonthDate, DimTime.Year, DimTime.QuarterDate, DimTime.YearDate, dimSiteLevelKey.Level2Key, dimSiteLevelKey.Level2Description, dimSiteLevelKey.Level1Key, 
                         dimSiteLevelKey.Level1Description, dimSiteLevelKey.Level3Description AS Division, dimProduct.ProdLineKey, dimProduct.ProdLineDescription AS ProdLine, dimProduct.CategoryKey, 
                         dimProduct.CategoryDescription AS Category, dimProduct.ProdGroupKey, dimProduct.ProdGroupDescription AS Prodgrp, dimItemFamilyCode.ItemFamilyCodeDescription AS Family, 
                         dimSalesTerritory.SalesTerritoryKey, dimSalesTerritory.SalesTerritoryDescription AS Territory, dimGeographic.DomesticForeign, dimGeographic.State, dimGeographic.CountryCode, 
                         dimGeographic.SubRegionKeyAlternative as SubRegion, dimGeographic.RegionKeyAlternative as Region, 0 AS GrossSales, factPlanSales.GrossAmtPlan AS AopSales
FROM            factPlanSales INNER JOIN
                         DimTime ON factPlanSales.InvoiceDateKey = DimTime.TimeKey INNER JOIN
                         dimSiteLevelKey ON factPlanSales.SiteKey = dimSiteLevelKey.LevelId INNER JOIN
                         dimProduct ON factPlanSales.ProdCodeSlKey = dimProduct.ProdCodeSlKey INNER JOIN
                         dimSalesTerritory ON factPlanSales.SalesTerritoryKey = dimSalesTerritory.SalesTerritoryKey INNER JOIN
                         dimGeographic ON factPlanSales.GeographicKey = dimGeographic.GeographicKey INNER JOIN
                         dimItemFamilyCode ON factPlanSales.ItemFamilyCodeKey = dimItemFamilyCode.ItemFamilyCodeKey
WHERE        (factPlanSales.SiteKey IN (1, 2, 4)) AND (DimTime.Year &gt;= 2015)
)

Select Date, MonthDate,QuarterDate,YearDate, Division, ProdLine, Category,Prodgrp, Family, Territory, DomesticForeign, State, CountryCode, Region, SubRegion,
Sum(GrossSales) as GrossSales, Sum(AopSales) as AopSales
 from cteData
 Group By Date, MonthDate,QuarterDate,YearDate, Division, ProdLine, Category,Prodgrp, Family, Territory, DomesticForeign, State, CountryCode,Region, SubRegion</property>
