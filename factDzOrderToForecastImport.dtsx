                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @MonthDateFrom as  date=Null
,@MonthDateTo as  date =NULL
 ,@DivisionKey as int=1

SET @MonthDateFrom='01/01/2016';
SET @MonthDateTo='12/31/2019';
--SET @MonthDateFrom= isnull(@MonthDateFrom, DATEADD(Month, DATEDIFF(Month, 0, GETDATE()) - 11, 0))
--SET @MonthDateTo= isnull(@MonthDateTo, DATEADD(Month, DATEDIFF(Month, 0, GETDATE()) - 0, 0))
Set @DivisionKey=ISNULL(@DivisionKey,1)


-- here are top 20 as of 2015
--('183605','183365','183141','183060','183072','183154','183838','183143','183063','185606','183606','183646','183772','183071','183070','183651','183142','102078','183661','173519')




Begin

		WITH cteOrders as
			(
				SELECT        DimTime.Year
					, factCustomerOrders.Sitekey
							, DimTime.Month
					, DimTime.MonthName
					,DimTime.MonthDate
					, dimGeographic.DomesticForeign
					, dimItem.ItemKeyAlternative
					, dimItem.ItemNoDescription
					, dimProduct.CategoryKey
					, dimProduct.CategoryDescription
					, dimProduct.ProdLineKey
					, dimProduct.ProdLineDescription
					, dimProduct.ProdGroupKey
					, dimProduct.ProdGroupDescription
					, dimItemFamilyCode.ItemFamilyCodeKey
					, dimItemFamilyCode.ItemFamilyCodeDescription
					, dimItem.ItemAbcCodeKeyAlternative  as AbcCode
					, dimItem.PlannerCodeKeyAlternative 
					,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000') AS ItemPlannerCodeDescription

					, dimItem.StdCost

					, SUM(factCustomerOrders.QtyOrdered)as QtyOrdered
					,0 as QtyForecast
					,0 as QtyForecastMfg
					,0 as QtyProduced
                                                                                ,1 as OnOrderForecast
					FROM            factCustomerOrders INNER JOIN
                         DimTime ON factCustomerOrders.DueDateKey = DimTime.TimeKey INNER JOIN
                         dimItem ON factCustomerOrders.ItemKey = dimItem.ItemKey INNER JOIN
                         dimProduct ON factCustomerOrders.ProdCodeSlKey = dimProduct.ProdCodeSlKey INNER JOIN
                         dimOrderEndType ON factCustomerOrders.Sitekey = dimOrderEndType.SiteKey AND factCustomerOrders.OrderEndKey = dimOrderEndType.OrderEndKey INNER JOIN
                         dimItemFamilyCode ON factCustomerOrders.ItemFamilyCodeKey = dimItemFamilyCode.ItemFamilyCodeKey LEFT OUTER JOIN
                         dimGeographic ON factCustomerOrders.GeographicKey = dimGeographic.GeographicKey LEFT OUTER JOIN
                         dimItemPlannerCode ON dimItem.SiteKey = dimItemPlannerCode.DivisionKey AND dimItem.PlannerCodeKeyAlternative = dimItemPlannerCode.ItemPlannerCodeKeyAlternative

				--where dimItem.ItemKeyAlternative in ('183605','183365','183141','183060','183072','183154','183838','183143','183063','185606','183606','183646','183772','183071','183070','183651','183142','102078','183661','173519')
				GROUP BY DimTime.Year
					, factCustomerOrders.Sitekey
					, DimTime.Month
					, DimTime.MonthName
					,DimTime.MonthDate
					
					, dimGeographic.DomesticForeign

					, dimItem.ItemKeyAlternative
					, dimItem.ItemNoDescription
					, dimProduct.CategoryKey
					, dimProduct.CategoryDescription
					, dimProduct.ProdLineKey
					, dimProduct.ProdLineDescription
					, dimProduct.ProdGroupKey
					, dimProduct.ProdGroupDescription
					, dimItemFamilyCode.ItemFamilyCodeKey
					, dimItemFamilyCode.ItemFamilyCodeDescription
					,dimItem.ItemAbcCodeKeyAlternative
					,dimOrderEndType.OrderEndDescription
					, dimItem.PlannerCodeKeyAlternative 
					,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000') 
					,dimItem.StdCost

				HAVING      (factCustomerOrders.Sitekey = @DivisionKey) 
					--AND (DimTime.Year in (SELECT     * FROM          dbo.BI_Split(@Year, ',')))
					--AND (DimTime.Month in (SELECT   * FROM      dbo.BI_Split(@Month, ',')))
					and DimTime.MonthDate between @MonthDateFrom and @MonthDateTo
					AND (dimOrderEndType.OrderEndDescription = N'Sales')
			)
			, cteForecast as
			(
			
			SELECT      factForecastSales.Year
				, factForecastSales.SiteKey
			
				, DimTime.Month
				, DimTime.MonthName
				,DimTime.MonthDate
				, dimGeographic.DomesticForeign
				, dimItem.ItemKeyAlternative
				, dimItem.ItemNoDescription
				, dimProduct.CategoryKey
				, dimProduct.CategoryDescription
				, dimProduct.ProdLineKey
				, dimProduct.ProdLineDescription
				, dimProduct.ProdGroupKey
				, dimProduct.ProdGroupDescription
				, dimItemFamilyCode.ItemFamilyCodeKey
				, dimItemFamilyCode.ItemFamilyCodeDescription
				 ,dimItem.ItemAbcCodeKeyAlternative as AbcCode
				, dimItem.PlannerCodeKeyAlternative
				,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000') AS ItemPlannerCodeDescription
				, dimItem.StdCost

				,0 as QtyOrdered
				, SUM(factForecastSales.QtyForecast) AS QtyForecast
				,0 as QtyForecastMfg
				,0 as QtyProduced
				  ,1 as OnOrderForecast
			FROM            dimItemFamilyCode INNER JOIN
                         factForecastSales INNER JOIN
                         DimTime ON factForecastSales.InvoiceDateKey = DimTime.TimeKey ON dimItemFamilyCode.ItemFamilyCodeKey = factForecastSales.ItemFamilyCodeKey LEFT OUTER JOIN
                         dimGeographic ON factForecastSales.GeographicKey = dimGeographic.GeographicKey LEFT OUTER JOIN
                         dimItemPlannerCode RIGHT OUTER JOIN
                         dimItem ON dimItemPlannerCode.DivisionKey = dimItem.SiteKey AND dimItemPlannerCode.ItemPlannerCodeKeyAlternative = dimItem.PlannerCodeKeyAlternative ON 
                         factForecastSales.ItemKey = dimItem.ItemKey LEFT OUTER JOIN
                         dimProduct ON factForecastSales.ProdCodeSlKey = dimProduct.ProdCodeSlKey

				--Where ItemKeyAlternative in ('183605','183365','183141','183060','183072','183154','183838','183143','183063','185606','183606','183646','183772','183071','183070','183651','183142','102078','183661','173519')

				GROUP BY factForecastSales.Year
					, factForecastSales.SiteKey
					, DimTime.Month
					, DimTime.MonthName
					,DimTime.MonthDate
					, dimGeographic.DomesticForeign
					, dimItem.ItemKeyAlternative
					, dimItem.ItemNoDescription
					, dimProduct.CategoryKey
					, dimProduct.CategoryDescription
					, dimProduct.ProdLineKey
					, dimProduct.ProdLineDescription
					, dimProduct.ProdGroupKey
					, dimProduct.ProdGroupDescription
					, dimItemFamilyCode.ItemFamilyCodeKey
					, dimItemFamilyCode.ItemFamilyCodeDescription
					,dimItem.ItemAbcCodeKeyAlternative
					, dimItem.PlannerCodeKeyAlternative
					,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000') 

					,dimItem.StdCost
				Having    (factForecastSales.SiteKey = @DivisionKey)

				and DimTime.MonthDate between @MonthDateFrom and @MonthDateTo
				 AND (SUM(factForecastSales.QtyForecast) &gt; 0)
			
			) 
			
			 ,cteForecastMfg as
			(
			
			SELECT      factForecastMfg.Year
				, factForecastMfg.DivisionKey
				, DimTime.Month
				, DimTime.MonthName
				,DimTime.MonthDate
				, dimGeographic.DomesticForeign
				, dimItem.ItemKeyAlternative
				, dimItem.ItemNoDescription
				, dimProduct.CategoryKey
				, dimProduct.CategoryDescription
				, dimProduct.ProdLineKey
				, dimProduct.ProdLineDescription
				, dimProduct.ProdGroupKey
				, dimProduct.ProdGroupDescription
				, dimItemFamilyCode.ItemFamilyCodeKey
				, dimItemFamilyCode.ItemFamilyCodeDescription

				 ,dimItem.ItemAbcCodeKeyAlternative as AbcCode
				, dimItem.PlannerCodeKeyAlternative
				,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000') AS ItemPlannerCodeDescription
				, dimItem.StdCost

				,0 as QtyOrdered
				,0 as QtyForecast
				,SUM(factForecastMfg.QtyForecast) AS QtyForecastMfg
				,0 as QtyProduced
				  ,1 as OnOrderForecast
			FROM            dimItemFamilyCode INNER JOIN
                         factForecastMfg INNER JOIN
                         DimTime ON factForecastMfg.InvoiceDateKey = DimTime.TimeKey ON dimItemFamilyCode.ItemFamilyCodeKey = factForecastMfg.ItemFamilyCodeKey LEFT OUTER JOIN
                         dimGeographic ON factForecastMfg.GeographicKey = dimGeographic.GeographicKey LEFT OUTER JOIN
                         dimItemPlannerCode RIGHT OUTER JOIN
                         dimItem ON dimItemPlannerCode.DivisionKey = dimItem.SiteKey AND dimItemPlannerCode.ItemPlannerCodeKeyAlternative = dimItem.PlannerCodeKeyAlternative ON 
                         factForecastMfg.ItemKey = dimItem.ItemKey LEFT OUTER JOIN
                         dimProduct ON factForecastMfg.ProdCodeSlKey = dimProduct.ProdCodeSlKey


				
				GROUP BY factForecastMfg.Year
					, factForecastMfg.DivisionKey
					, DimTime.Month
					, DimTime.MonthName
					,DimTime.MonthDate
					, dimGeographic.DomesticForeign
					, dimItem.ItemKeyAlternative
					, dimItem.ItemNoDescription
					, dimProduct.CategoryKey
					, dimProduct.CategoryDescription
					, dimProduct.ProdLineKey
					, dimProduct.ProdLineDescription
					, dimProduct.ProdGroupKey
					, dimProduct.ProdGroupDescription
					, dimItemFamilyCode.ItemFamilyCodeKey
					, dimItemFamilyCode.ItemFamilyCodeDescription
					,dimItem.ItemAbcCodeKeyAlternative
					, dimItem.PlannerCodeKeyAlternative
					,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000') 
					,dimItem.StdCost

				Having     (factForecastMfg.DivisionKey =@DivisionKey)
				and DimTime.MonthDate between @MonthDateFrom and @MonthDateTo
				AND (SUM(factForecastMfg.QtyForecast) &gt; 0)
			
			) 
			,cteProduced as 
			(
			SELECT  
				 DimTime.Year
				, MT.site_ref as DivisionKey
				, DimTime.Month
				, DimTime.MonthName
				, DimTime.MonthDate
				,'Produced' as DomesticForeign
				, dimItem.ItemKeyAlternative
					, dimItem.ItemNoDescription
					, dimProduct.CategoryKey
					, dimProduct.CategoryDescription
					, dimProduct.ProdLineKey
					, dimProduct.ProdLineDescription
					, dimProduct.ProdGroupKey
					, dimProduct.ProdGroupDescription
					, dimItemFamilyCode.ItemFamilyCodeKey
					, dimItemFamilyCode.ItemFamilyCodeDescription
					,dimItem.ItemAbcCodeKeyAlternative
					, dimItem.PlannerCodeKeyAlternative
					,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000')  AS ItemPlannerCodeDescription
					,dimItem.StdCost
					,0 as QtyOrdered
					,0 as QtyForecast
					,0 AS QtyForecastMfg
					, SUM(MT.qty) AS QtyProduced
				  ,0 as OnOrderForecast
				FROM     DimTime RIGHT OUTER JOIN
             dimItemPlannerCode RIGHT OUTER JOIN
             BIRMCSIDBS.BET_App.dbo.matltran_mst AS MT LEFT OUTER JOIN
             dimItem ON MT.site_ref = dimItem.SiteKeyAlternative AND MT.item = dimItem.ItemKeyAlternative LEFT OUTER JOIN
             dimItemFamilyCode ON dimItem.SiteKey = dimItemFamilyCode.DivisionKey AND dimItem.ItemFamilyCodeKey = dimItemFamilyCode.ItemFamilyCodeKey LEFT OUTER JOIN
             dimProduct ON dimItem.SiteKey = dimProduct.DivisionKey AND dimItem.ProdCodeSlKey = dimProduct.ProdCodeSlKey ON dimItemPlannerCode.ItemPlannerCodeKeyAlternative = dimItem.PlannerCodeKeyAlternative AND dimItemPlannerCode.DivisionKey = dimItem.SiteKey ON DimTime.Date = MT.trans_date

				WHERE   (MT.ref_type = N'J') 
				AND (MT.trans_type = N'F')
				AND (dimItem.ItemKeyAlternative IN
                             (SELECT        item                              FROM            BIRMCSIDBS.BET_App.dbo.coitem_mst AS coitem_mst_1))


				GROUP BY DimTime.Year
					, MT.site_ref
					, DimTime.Month
					, DimTime.MonthName
					,DimTime.MonthDate
						
					, dimItem.ItemKeyAlternative
					, dimItem.ItemNoDescription
					, dimProduct.CategoryKey
					, dimProduct.CategoryDescription
					, dimProduct.ProdLineKey
					, dimProduct.ProdLineDescription
					, dimProduct.ProdGroupKey
					, dimProduct.ProdGroupDescription
					, dimItemFamilyCode.ItemFamilyCodeKey
					, dimItemFamilyCode.ItemFamilyCodeDescription
					,dimItem.ItemAbcCodeKeyAlternative
					, dimItem.PlannerCodeKeyAlternative
					,COALESCE (dimItemPlannerCode.ItemPlannerCodeDescription, 
							  dimItem.PlannerCodeKeyAlternative, '000') 
					,dimItem.StdCost
				HAVING   (DimTime.MonthDate between @MonthDateFrom and @MonthDateTo)
				
			)
			,cteOrdersForecast as
			(
			
				Select *
				From cteOrders
				Union all
				Select *
				From cteForecast
				Union all
				Select *
				From cteForecastMfg
				Union all
				Select *
				From cteProduced
                                                             --   Where (ItemKeyAlternative in 
                                                         --                             (Select                                                                                                              --ItemKeyAlternative                                                                                          --From cteOrders))
                                                   --     or 
                                               --  (ItemKeyAlternative in 
                                                                                  --    (Select                                                                                                          --         --ItemKeyAlternative                                                                                          --From cteForecast))
			
			)
			--MAPE = (Absolute Value(Actual – Forecast) / Actual) x 100
			--Mean Absolute Error (MAE)  Sum of Absolute Value(Actual – Forecast)/number of Rows


			Select  
			Year
				
				
				,MonthDate
			,Month
				,isnull(DomesticForeign,'Domestic') as DomesticForeign
				,ProdlineDescription
				,CategoryDescription
				,ProdGroupDescription
				,ItemKeyAlternative as Item
				,ItemNoDescription
				,AbcCode
				, PlannerCodeKeyAlternative
				, ItemPlannerCodeDescription
				,ItemFamilyCodeDescription
				
				,SUM(QtyOrdered)as  QtyOrdered
				,SUM(QtyForecast) as  QtyForecast
				,SUM(QtyForecastMfg) as QtyForecastMfg
				,Sum(QtyProduced) as QtyProduced
				,abs(SUM(QtyOrdered-QtyForecast)) as AbsDiff
				,abs(SUM(QtyOrdered-QtyForecastMfg)) as AbsDiffMfg
				
				

			From cteOrdersForecast	
			

			Group  by Year
				,Month
				,MonthDate
				,DomesticForeign
				, CategoryDescription
				,ProdlineDescription
				,ProdGroupDescription
				,ItemFamilyCodeDescription
				,ItemKeyAlternative
				,ItemNoDescription
				,AbcCode
				,PlannerCodeKeyAlternative
				,ItemPlannerCodeDescription
				
				
			
		
		
		
		




			
End</property>
